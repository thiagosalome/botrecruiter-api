    "profiles": {
      "collection": "profile",
      "via": "user"
    },
    "location": {
      "model": "location",
      "via": "user"
    },
    "languages": {
      "collection": "language",
      "via": "user"
    },
    "educations": {
      "collection": "education",
      "via": "user"
    },
    "awards": {
      "collection": "award",
      "via": "user"
    },
    "works": {
      "collection": "work",
      "via": "user"
